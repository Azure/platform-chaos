    "@types/node": "^10.10.1",
    "@types/shimmer": "^1.0.1",
    "@types/sinon": "^5.0.2",
    "@types/uuid": "^3.4.4",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^5.2.0",
    "sinon": "^6.2.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
