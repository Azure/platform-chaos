{
  "name": "platform-chaos",
  "version": "1.0.0",
  "description": "A node sdk for building services capable of injecting chaos into PaaS offerings",
  "main": "dist/index.js",
  "scripts": {
    "lint": "npx tslint -c tslint.json 'src/**/*.ts'",
    "pretest": "npm run cleanup && npm run compile",
